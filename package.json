  "scripts": {
    "build": "npm-run-all clearBuildDir webpack:local",
    "build:dev": "npm-run-all lint clearBuildDir webpack:dev",
    "build:prod": "npm-run-all lint clearBuildDir webpack:prod",
    "clearBuildDir": "mkdir -p build && rm -rf build/*",
    "lint": "eslint \"src/**/*.tsx\" \"src/**/*.ts\"",
    "start": "webpack serve --progress --config webpack.local.js --mode development --live-reload",
    "test": "echo \"Error: no test specified\" && exit 0",
    "webpack:dev": "webpack --config webpack.dev.js --mode development",
    "webpack:local": "webpack --config webpack.local.js --mode development",
    "webpack:prod": "webpack --config webpack.prod.js --mode production"
  },
